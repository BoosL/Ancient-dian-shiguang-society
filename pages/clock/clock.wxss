@import "../footer-template/footer-template.wxss";

/* 引用模块样式 */

page {
  height: 100%;
}

.container {
  position: relative;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.banck {
  position: absolute;
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
}

swiper-item view {
  height: 100%;
}

.swiper-box {
  width: 100%;
  height: 1130rpx;
}

.log {
  position: absolute;
  left: 38rpx;
  top: 32rpx;
  width: 128rpx;
  height: 80rpx;
  background-image: url('http://gz.wauwo.net/miniAPP/resources/img/weixin/a6.png');
  background-size: 100% 100%;
}

.rule {
  position: absolute;
  right: 46rpx;
  top: 76rpx;
  width: auto;
  height: 36rpx;
  border-bottom: 1px solid #fff;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  font-weight: 600;
  z-index: 9999;
}

.show {
  position: absolute;
  top: 200rpx;
  left: 200rpx;
  z-index: 99;
  width: 500rpx;
  height: 500rpx;
}

.Sixborder {
  position: absolute;
  left: 80rpx;
  top: 94rpx;
  width: 587rpx;
  height: 694rpx;
}

.boxF, .boxS, .boxT, .overlay {
  width: 554rpx;
  height: 640rpx;
  overflow: hidden;
}

.boxF {
  position: absolute;
  top: 120rpx;
  left: 96rpx;
  transform: rotate(120deg);
  float: left;
  -ms-transform: rotate(120deg);
  -moz-transform: rotate(120deg);
  -webkit-transform: rotate(120deg);
}

.boxS {
  transform: rotate(-60deg);
  -ms-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
}

.boxT {
  transform: rotate(-60deg);
  background: no-repeat 50% center;
  -ms-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
  visibility: visible;
  background-color: #fff;
  background-size: 100% 100%;
  white-space: nowrap;
  width: 554rpx;
}

.boxFF, .boxSS, .boxTT, .overlay {
  width: 520rpx;
  height: 640rpx;
  overflow: hidden;
}

.boxFF {
  position: absolute;
  top: 0;
  left: -2rpx;
  transform: rotate(120deg);
  float: left;
  margin-left: 10px;
  -ms-transform: rotate(120deg);
  -moz-transform: rotate(120deg);
  -webkit-transform: rotate(120deg);
}

.boxSS {
  transform: rotate(-60deg);
  -ms-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
}

.boxTT {
  transform: rotate(-60deg);
  background: no-repeat 50% center;
  -ms-transform: rotate(-60deg);
  -moz-transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
  visibility: visible;
  background-size: 100% 100%;
  overflow: hidden;
  text-decoration: none;
}

.change {
  float: left;
  width: 560rpx;
  height: auto;
  text-align: center;
}

.title, .titles {
  width: auto;
  display: flex;
  flex-direction: column;
  margin-top: 160rpx;
  font-size: 60rpx;
  color: #47109f;
  font-weight: 600;
  text-align: center;
}

.title {
  margin-top: 160rpx;
  margin-left: 4rpx;
}

.Lo, .Lt, .Ro, .Rt {
  width: auto;
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
  text-align: center;
  margin-top: 20rpx;
}

.Lo {
  margin-top: 40rpx;
}

.oo {
  position: absolute;
  bottom: 150rpx;
  color: #fff;
  font-size: 30rpx;
}

.choose {
  float: left;
  width: 750rpx;
  height: auto;
  text-align: center;
}

.LeftC {
  display: flex;
  flex-direction: column;
  float: left;
  margin-left: 80rpx;
}

.LCo, .LCt, .RCo, .RCt {
  border: 1px solid #fff;
  border-radius: 29rpx;
  width: 209rpx;
  height: 59rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 59rpx;
  margin-top: 50rpx;
  background-color: #f39801;
}

.RightC {
  display: flex;
  flex-direction: column;
  float: right;
  margin-right: 80rpx;
}

.Interpreter, .judge {
  width: 420rpx;
  font-size: 22rpx;
  color: #999;
  display: flex;
  flex-direction: column;
  margin-left: 50rpx;
  margin-top: 0;
}

.judge {
  display: flex;
  flex-direction: column;
  margin-left: 116rpx;
}

.logs {
  position: absolute;
  top: 130rpx;
  left: 50%;
  transform: translate(-50%, 0);
  width: 209rpx;
  height: 175rpx;
}

.tex, .texs, .texts {
  position: absolute;
  color: #333;
  z-index: 10;
  width: 100%;
  text-align: center;
  left: 50%;
  transform: translate(-50%, 0);
}

.tex {
  top: 344rpx;
  font-size: 44rpx;
}

.texs {
  top: 464rpx;
  font-size: 36rpx;
}

.texts {
  font-size: 16rpx;
  top: 490rpx;
  font-weight: 600;
  color: #666;
}

.lButton, .rButton, .runBtn {
  position: absolute;
  bottom: 214rpx;
  color: #fff;
  border: 1px solid #fff;
  border-radius: 29rpx;
  width: 209rpx;
  height: 59rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 59rpx;
}

.lButton {
  left: 80rpx;
}

.rButton {
  right: 80rpx;
}

.runBtn {
  left: 269rpx;
}

.swiper-tab {
  position: absolute;
  bottom: 132rpx;
  width: 100%;
  text-align: center;
}

.swiper-tab-list {
  border-radius: 50%;
  width: 20rpx;
  height: 20rpx;
  background-color: #fff;
  display: inline-block;
  margin-left: 10rpx;
}

.on {
  background-color: #fff;
  opacity: 0.5;
}
